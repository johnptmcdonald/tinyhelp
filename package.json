{
  "name": "tinyhelp",
  "version": "0.0.0-semantically-released",
  "description": "Tiny helpers for lazy developers",
  "main": "dist/index.js",
  "scripts": {
    "commit": "git-cz",
    "report-coverage": "codecov",
    "cover": "nyc npm t",
    "test": "nyc --reporter=lcov --reporter=text-summary mocha src/tests/*.js --require babel-core/register",
    "semantic-release": "semantic-release",
    "build": "babel --copy-files --out-dir dist src --ignore tests",
    "prebuild": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/johnptmcdonald/tinyhelp.git"
  },
  "keywords": [
    "example",
    "simple"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "author": "johnptmcdonald <johnptmcdonald@gmail.com> (http://www.johnptmcdonald.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/johnptmcdonald/tinyhelp/issues"
  },
  "homepage": "https://github.com/johnptmcdonald/tinyhelp#readme",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-loader": "8.1.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "babel-register": "6.26.0",
    "chai": "4.2.0",
    "codecov": "3.6.5",
    "commitizen": "^4.0.4",
    "cz-conventional-changelog": "3.1.0",
    "ghooks": "2.0.4",
    "istanbul": "0.4.5",
    "jsdoc": "3.6.4",
    "json-loader": "0.5.7",
    "mocha": "7.1.1",
    "nyc": "15.0.1",
    "rimraf": "3.0.2",
    "semantic-release": "^19.0.3",
    "webpack": "4.42.1",
    "webpack-cli": "3.3.11"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm test"
    }
  },
  "babel": {
    "presets": [
      "env",
      "stage-2"
    ]
  }
}
